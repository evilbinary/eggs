import os

Import('env')

source=[]

source+= [
    'alldevices.c',                                                  
    'avdevice.c',                                                    
    'utils.c',                                                       

]

if env.get('MYLIB'):
    source.append(env.get('MYLIB'))


source+= [
    ]+env['LIBC']

env.Library('avdevice', source,
    CFLAGS='$CFLAGS  $LIBCFLAGS -Iapp/libffmpeg/ ',)
