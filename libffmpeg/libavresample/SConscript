import os

Import('env')

source=[]

source+= Glob('*.c')

if env.get('MYLIB'):
    source.append(env.get('MYLIB'))


source+= [
    ]+env['LIBC']

env.Library('avresample', source,
    CFLAGS='$CFLAGS  $LIBCFLAGS -Iapp/libffmpeg/ ',)
